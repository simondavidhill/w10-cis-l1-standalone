; 00000002 = Preview Build - Fast
; 00000004 = Preview Build - Slow
; 00000008 = Release Preview
; 00000010 = Semi-Annual Channel (Targeted)
; 00000020 = Semi-Annual Channel (CIS)
"BranchReadinessLevel"=dword:00000020

[HKEY_LOCAL_MACHINE\SOFTWARE\Policies\Microsoft\Windows\WindowsUpdate]
; <deleted> = (Default)
; 0000000 = Off
; 0000001 = On (CIS)
"DeferFeatureUpdates"=dword:0000001

; 18.9.102.1.3 (L1) Ensure 'Select when Quality Updates are received' is set to 'Enabled: 0 days'
[HKEY_LOCAL_MACHINE\SOFTWARE\Policies\Microsoft\Windows\WindowsUpdate]
; <deleted> = (Default)
; 0000000 = Off
; 0000001 = On (Default / CIS)
"DeferQualityUpdates"=dword:0000001
[HKEY_LOCAL_MACHINE\SOFTWARE\Policies\Microsoft\Windows\WindowsUpdate]
; <deleted> = (Default)
; 0000000 = 0 days (min / Default / CIS)
; 00000b4 = 180 days (middle)
; 000016d = 365 days (max)
"DeferQualityUpdatesPeriodInDays"=dword:0000000

; 18.9.102.2 (L1) Ensure 'Configure Automatic Updates' is set to 'Enabled'
[HKEY_LOCAL_MACHINE\Software\Policies\Microsoft\Windows\WindowsUpdate\AU]
; <deleted> = (Default)
; 0000000 = Off (CIS)
; 0000001 = On
"NoAutoUpdate"=dword:0000000

; 18.9.102.3 (L1) Ensure 'Configure Automatic Updates: Scheduled install day' is set to '0 - Every day'
[HKEY_LOCAL_MACHINE\Software\Policies\Microsoft\Windows\WindowsUpdate\AU]
; <deleted> = (Default)
; 0000000 = Every day (CIS)
; 0000001 = Every Sunday
; 0000002 = Every Monday
; 0000003 = Every Tuesday
; 0000004 = Every Wednesday
; 0000005 = Every Thursday
; 0000006 = Every Friday
; 0000007 = Every Saturday
"ScheduledInstallDay"=dword:0000000

; 18.9.102.4 (L1) Ensure 'No auto-restart with logged on users for scheduled automatic updates installations' is set to 'Disabled'
[HKEY_LOCAL_MACHINE\Software\Policies\Microsoft\Windows\WindowsUpdate\AU]
; <deleted> = (Default)
; 0000000 = Off (Default / CIS)
; 0000001 = On
"NoAutoRebootWithLoggedOnUsers"=dword:0000000

; 18.9.102.5 (L1) Ensure 'Remove access to “Pause updates” feature' is set to 'Enabled'
[HKEY_LOCAL_MACHINE\SOFTWARE\Policies\Microsoft\Windows\WindowsUpdate]
; <deleted> = (Default)
; 0000000 = Off (Default)
; 0000001 = On (CIS)
"SetDisablePauseUXAccess"=dword:0000001

; 19.1.3 Personalization (formerly Desktop Themes)

; 19.1.3.1 (L1) Ensure 'Enable screen saver' is set to 'Enabled'
[HKEY_CURRENT_USER\Software\Policies\Microsoft\Windows\Control Panel\Desktop]
; <deleted> = (Default)
; 0 = Off
; 1 = On (CIS)
"ScreenSaveActive"="1"

; 19.1.3.3 (L1) Ensure 'Password protect the screen saver' is set to 'Enabled'
[HKEY_CURRENT_USER\Software\Policies\Microsoft\Windows\Control Panel\Desktop]
; <deleted> = (Default)
; 0 = Off
; 1 = On (CIS)
"ScreenSaverIsSecure"="1"

; 19.1.3.4 (L1) Ensure 'Screen saver timeout' is set to 'Enabled: 900 seconds or fewer, but not 0'
[HKEY_CURRENT_USER\Software\Policies\Microsoft\Windows\Control Panel\Desktop]
; <deleted> = (Default)
; 900 = 900 seconds (CIS)
"ScreenSaveTimeOut"="900"


